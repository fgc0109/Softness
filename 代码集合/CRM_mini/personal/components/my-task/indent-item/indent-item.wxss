  align-items: center;
  font-size: 28rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  text-align: left;
  color: #0082fc;
}

.common-img-sty {
  margin-left: 10rpx;
  width: 32rpx;
  height: 32rpx;

}