.indicator-class>view:not(:last-child) {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 34rpx;
  font-weight: 400;
  text-align: left;
  color: #7b7b7b;
}

.indicator-class {
  background: #ffffff;
  margin-top: 24rpx;
  padding: 34rpx 24rpx;
  border-radius: 20rpx;
}

.border {
  width: 327px;

  border: 0.5px solid #eaebef;
}

.attachment {
  margin-top: 30rpx;
  display: flex;
