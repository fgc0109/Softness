
.time-line {
  display: grid;
  grid-template-columns: 12% 88%;
  margin: 20rpx;
}

.current-time {
  width: 32rpx;
  height: 32rpx;
  background: #0082fc;
  border-radius: 50%;
  float: right;
  margin-right: -16rpx;
}

.operate {
  display: block;
  padding-left: 40rpx;
}

.vertical-line {
  height: 110%;
  border-right: 1px dashed #D9D9D9;
}

.operate-content {
  font-size: 28rpx;
  font-weight: 550;
  color: #555555;
}

.operate-name {
  margin: 20rpx 0;
  color: #555555;
}

.operate-time {
  color: #B4B4B4;
}

.no-popup {
  padding: 20rpx 0 300rpx 20rpx;
}

.no-code {
  width: 170rpx;
  height: 60rpx;
  font-size: 26rpx;
  line-height: 60rpx;
  padding: 0;
  margin-left: 20rpx;
  background-color: #fff;
}

.no-font {
  font-size: 24rpx;
  margin: 30rpx;
  color: #ccc;
}
.no-reson{
  margin-bottom: 0;
  margin-top: 50rpx;
  font-size: 28rpx;
  color: #646566;
}
.no-reson2{
  font-size: 24rpx;
  color: #ccc;
  margin-top: 10rpx;
  margin-bottom: 0;
}
.pointer-sty {
  display: inline-block;
  width: 16rpx;
  height: 16rpx;
  border-top: 3rpx solid #969799;
  border-right: 3rpx solid #969799;
  transform: rotate(45deg);
  margin-left: 8rpx;
  margin-right: 14rpx;
}

.strong-sty {
  color: #969799 !important;
  font-size: 14px !important;
}
.toAuditBtn{
  background-color: #0082fc !important;
  width: 80%;
  border-radius: 45rpx;
  font-size: 32rpx;
  color: #fff;
}