  overflow: hidden;
  text-overflow: ellipsis;
}

.name_list_sty {
  /* height: 100%; */
  display: flex;
  align-items: center;
  justify-content: space-evenly;
  flex-wrap: wrap;
  text-align: left;
  padding: 0 20rpx;
  margin: 120rpx 0 20rpx 0;
}

.icon_sty {
  width: 40rpx;
  height: 40rpx;
}

.name_list_sty>view {
  line-height: 50%;
}

.userInfo_sty {
  display: flex;
  align-items: center;
}