
page {
  background: #F5F7FB;
}

.contract-item {
  background: #FFFFFF;
  width: 95%;
  display: flex;
  flex-direction: column;
  border-radius: 20rpx;
  margin: 20rpx auto;
  font-size: 28rpx;
}

.audit {
  color: #FC561F;
  width: 95%;
  height: 80rpx;
  line-height: 80rpx;
  background: #FFF0EB;
  margin: 20rpx auto;
  font-size: 24rpx;
  border-radius: 20rpx;
  text-align: center;
}

/*
  头部区域
*/
.contract-head {
  display: flex;
  background: #FBFCFE;
  justify-content: space-between;
  padding: 20rpx;
  border-top-left-radius: 20rpx;
  border-top-right-radius: 20rpx;
}

.head-left {
  display: flex;
  align-items: center;
}

.head-left>text:nth-of-type(1) {
  color: #555555;
  font-size: 28rpx;
}

.head-right {
  display: flex;
  align-items: center;
  color: #555555;
  font-size: 24rpx;
}

.head-right>text:nth-of-type(1) {
  margin-right: 12rpx;
}

.common-img-sty {
  width: 32rpx;
  height: 32rpx;
  margin-left: 16rpx;
}

.passdot {
  width: 6px;
  height: 6px;
  background: #28c445;
  border-radius: 50%;
}

.notpass {
  width: 6px;
  height: 6px;
  background: #FA1B2A;
  border-radius: 50%;
}

.wait {
  width: 6px;
  height: 6px;
  background: #3C98FE;
  border-radius: 50%;
}

.contract-info {
  margin-top: 20rpx;
  padding: 20rpx;
  display: grid;
  grid-template-columns: 27% 73%;
  grid-gap: 20rpx;
  color: #555555;
  background: #FFFFFF;
  border-bottom-left-radius: 20rpx;
  border-bottom-right-radius: 20rpx;
}

.newcontract-info {
  margin-top: 20rpx;
  padding: 20rpx 20rpx 0 20rpx;
  display: grid;
  grid-template-columns: 27% 73%;
  grid-gap: 20rpx;
  color: #7b7b7b;
  background: #FFFFFF;
  border-bottom-left-radius: 20rpx;
  border-bottom-right-radius: 20rpx;
}

.contract-info :nth-child(2n+1) {
  color: #7B7B7B;
}

.contract-price {
  color: #FA7A42 !important;
  font-weight: bold;
}

.contract-line {
  width: 100%;
  margin: 20rpx auto;
  border-bottom: 1px solid #EAEBEF;
}

.basic-clue-info {
  width: 100%;
  border-radius: 20rpx;
  margin: 20rpx auto;
  background: #FFFFFF;
  display: grid;
  grid-template-columns: 30% 65%;
  grid-gap: 28rpx;
  color: #7B7B7B;
}

.basic-clue-info :nth-child(2n) {
  text-align: right;
  color: #555555;
  font-size: 28rpx;
}

.basic-info {
  width: 90%;
  border-radius: 20rpx;
  margin: 20rpx auto;
  padding: 20rpx;
  background: #FFFFFF;
  display: grid;
  grid-template-columns: repeat(2, 1fr);
  grid-gap: 28rpx;
  color: #7B7B7B;
}

.basic-info :nth-child(2n) {
  text-align: right;
  color: #555555;
}

.clue-info {
  width: 90%;
  border-radius: 20rpx;
  margin: 20rpx auto;
  /* margin-bottom: 92rpx; */
  padding: 20rpx;
  background: #FFFFFF;
  grid-gap: 28rpx;
  color: #7B7B7B;
}

.url-contract {
  margin-bottom: 20rpx;
  font-size: 26rpx;
  color: #0082FC !important;
}

.operate-btn {

  position: fixed;
  bottom: 0;
  width: 100%;
  height: 90rpx;
  background: #FFFFFF;
  color: #0F89FC;
  line-height: 90rpx;
  display: grid;
  grid-template-columns: repeat(2, 1fr);
  text-align: center;
  border-top: 1px solid #E5E6EB;
  font-size: 28rpx;
}

.operate-btn :nth-child(2n) {
  background: #0F89FC;
  color: #FFFFFF;
}

.common-item-textarea {
  padding: 18rpx;
  width: 618rpx;
  height: 180rpx;
  font-size: 28rpx;
  color: #7B7B7B;
  background: #F9FBFC;
}

.phone-error-code {
  margin: 20rpx 0;
  font-size: 24rpx;
  color: red;
}

/* 
操作记录
*/

.time-line {
  display: grid;
  grid-template-columns: 12% 88%;
  margin: 20rpx;
  /* padding: 24rpx 0; */
}

.current-time {
  width: 30rpx;
  height: 30rpx;
  background: #0082fc;
  border-radius: 50%;
  float: right;
  margin-right: -16rpx;
}

.operate {
  display: block;
  padding-left: 32rpx;
}

.vertical-line {
  height: 110%;
  border-right: 1px dashed #D9D9D9;
}

.operate-content {
  font-size: 28rpx;
  font-weight: 550;
  color: #555555;
}

.operate-name {
  font-size: 28rpx;
  margin: 20rpx 0;
  color: #555555;
}
.audit-txt{
  width: 100%;
  word-wrap: break-word;
}
.operate-time {
  font-size: 28rpx;
  color: #B4B4B4;
}

/* 
  订单信息
*/
.order-list {
  padding: 20rpx;
  margin-bottom: 184rpx;
  border-radius: 10rpx;
}

.order-info {
  width: 95%;
  padding: 0 20rpx;
  margin-top: 20rpx;
  background: #FFFFFF;
  border-radius: 20rpx;
}

.order-no {
  width: 100%;
  height: 92rpx;
  display: flex;
  align-items: center;
  background: #FBFCFE;
  color: #333333;
}

.line {
  width: 100%;
  border-bottom: 1px solid #EAEBEF;
  margin: 20rpx auto;
}

.spu-name {
  display: grid;
  grid-template-columns: 70% 30%;
  margin: 20rpx 0;
  color: #333333;
  font-weight: 550;
  font-size: 28rpx;
}

.spu-name :nth-child(2n) {
  text-align: right;
  font-weight: 400;
  color: #7B7B7B;
}

.spec {
  font-size: 24rpx;
  background: #C4E2FF;
  padding: 4rpx 16rpx;
  margin-right: 16rpx;
  border-radius: 8rpx;
  color: #4BA8FF;
}

.fee-info {
  display: grid;
  grid-template-columns: 40% 60%;
  grid-gap: 10rpx;
  margin-top: 20rpx;
  color: #7B7B7B;
  font-size: 28rpx;
}

.fee-info :nth-child(2n) {
  color: #555555 !important;
  text-align: right;
  padding-right: 5rpx;
}

.total-amount {
  display: flex;
  padding-bottom: 20rpx;
}

.total {
  border-top: 1px solid #EAEBEF;
  bottom: 92rpx;
  background: #FFFFFF;
  height: 100rpx;
  padding: 0 24px;
  /* margin-left: -10%;
  padding-right: 10%; */
  line-height: 100rpx;
  text-align: right;
  font-weight: 550;
}

.price {
  color: #FA7A42;
  font-weight: bold;
}

.empty-order {
  text-align: center;
  margin: 30rpx 0;
  color: #B4B4B4;
}

.tab-info {
  margin: 20rpx;
  border-radius: 20rpx;
  background: #FFFFFF;
  padding: 24rpx 0;
}

.radiussty {
  width: 16rpx;
  height: 16rpx;
  background: #0082fc;
  border-radius: 50%;
  display: inline-block;
  margin-right: 8rpx;
}

.typeface {
  font-size: 28rpx;
  font-weight: 700;
  text-align: left;
  color: #555555;

}

.number-df {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.sty-botton {
  margin-bottom: 20rpx;
}

.sty-state {
  margin-top: 10rpx;
}

.typefaceblue {
  /* display: inline-block; */
  width: 100%;
  text-align: center;
  background: #c4e2ff;
  border-radius: 8rpx;
  font-size: 22rpx;
  padding: 4rpx 22rpx;
  color: #4BA8FF;
}

.typefaceblue-box {
  margin: 4rpx 0 28rpx 0;
}

.srt-sty {
  font-size: 14px;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  text-align: left;
  color: #555555 !important;
}

.swiper-box {
  height: 900px;
}

.swiper-sty {
  /* margin-top: 48rpx; */
  width: 700rpx;
  height: 460rpx;
}

.dot-sty {
  margin: 0 auto;
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 55rpx;
}
.dot-sty-item {
  width: 8px;
  height: 8px;
  background: #e5e6e6;
  border-radius: 50%;
}

.dot-sty-state {
  width: 15px;
  height: 8px;
  background: #0082fc;
  border-radius: 4px;
}
.dot-sty-box{
  margin-bottom: 24rpx;
}

.url-contract{
  margin-bottom: 20rpx;
  font-size: 26rpx;
  color: #0082FC !important;
}