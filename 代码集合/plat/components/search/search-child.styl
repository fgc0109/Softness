.child-content
    width: 100%
    background: #fff
    position: relative;
    .filter-line
        width: 100%
        border-bottom: 1px solid #E5E5E5
        display: flex
        .filter-label
            width: 150px
            line-height:50px
            font-size 14px
            font-weight: 600
            color: #333
            text-align: center
            border-right: 1px solid #E5E5E5
            font-family: Microsoft YaHei, Microsoft YaHei-Bold;
        .filter-value
            max-width: 1042px
            margin-left: 15px
            display: flex
            flex-flow: row wrap
            align-items: center
            p
                color: #666
                font-size: 14px
                padding: 0 16px
                cursor pointer
            .active
                color: #36a6fe !important
    .search-box
        margin:15px auto;
        width:1200px;
        .left-side
            background:#fff;
            display:flex;
            flex-direction: column;
        
            .search-left   
                min-width: 200px;
                display:flex;
                flex-direction: column;
                margin-bottom:50px
                .search-left-header
                    background:#36A6FE;
                    padding:9px;
                    padding-left:20px;
                    color:#fff;
                    font-size: 18px;
                    font-weight:bold
                        .checked 
                            color: #FFFFFF !important;
                            background: #7AC4FF !important;
        .search-right
            background:#fff;
            flex:1
            .search-right-header
                font-size: 14px;
                border-bottom:2px solid #eee;
                padding:10px;   
            
            .tab-content
                padding:7px 0px
                .top
                    border-top:8px solid  #eee;
                    /* padding-bottom:15px */
                .content 
                    font-size:14px;
                    padding-top:7px; 
                    padding-bottom:7px; 
                    padding-left:15px;   
                    color:#333;        
                    display:flex;
                    -webkit-box-orient:vertical;
                    -webkit-line-clamp:1;   
                    .tab-items    
                        cursor:pointer;
                        display:flex;
                        color:#373D40;
                        align-items: center;
                        .bluecircle
                            display: inline-block;
                            width: 6px;
                            height: 6px;
                            background: #36a6fe;
                            border-radius: 50%;
                        .tab-title
                            width:550px;
                            padding-left:0;
                            overflow:hidden;
                            text-overflow:ellipsis;
                            white-space:nowrap;
                            color:#333
                        .time
                            margin-left:230px;
                            color:#999
                        .timeFile
                            margin-left:212px;
                            color:#999
            .err-tips
                margin-top: 150px;
                text-align: center;
                font-size: 14px;
                color: #333;
    .err-tips
        margin-top: 80px;
        padding-bottom: 60px
        text-align: center;
        font-size: 14px;
        color: #333;
    .pro-list
        width: 100%
        padding: 30px
        display:inline-grid;
        grid-template-columns: repeat(5,19%)
        grid-column-gap: 1%;
        grid-row-gap: 14px;
        grid-auto-rows:auto;
        .list-content
            width: 100%
            padding: 14px
            border: 1px solid #F4F4F6
            border-radius: 6px
            .item-img
                width: 100%
                height: 0
                padding-bottom: 140px
                overflow: hidden
                img
                    width: 100%
            .item-price
                margin-top: 14px
                span
                    font-size: 12px
                .big-price
                    font-size: 20px !important
            .pro-title {
                overflow hidden
                text-overflow ellipsis
                width 187px
                height 38px
            }
    .search-left .search-left-header~div
        padding:10px;
        padding-left:20px;
        cursor: pointer;
    .page-style
        text-align:center;
        height:80px;
        padding: 20px;
        margin-top: 40px;
    .sort-content
        width: 150px;
        height: 30px;
        position: absolute;
        right: 0px;
        top: -30px;
        bottom: 50px;
    .tit-sty 
        font-weight: 700 !important;
    .text-sty
        font-family: Microsoft YaHei, Microsoft YaHei-Bold;
        font-size: 14px;
        color: #666666;
        display: inline-block;
        margin-right: 15px;
    .text-sty:hover
        cursor: pointer;
    .active-sty
        color: #36a6fe !important;
        fon-size: 14px;
        font-weight: bold;
    .active
        color: #36a6fe !important;
        fon-size: 14px;
