.home_frame {
  max-width 100%
  min-width 1200px
  margin: 0 auto;
  background: #f2f4f5;
  overflow hidden

  .zuo{
        position: relative;
        width: 396px;
        height: 518px;
        top: 32px;
        left: -142px;
        object-fit: cover;
        min-width: 396px;
    }
  .you{
        width: 396px;
        height: 518px;
        position relative;
        top: 32px;
        left: -95px;
        object-fit: cover;
