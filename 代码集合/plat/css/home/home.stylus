    left -158px
    .specialbanner{
        width: 700px; 
        min-width: 700px;
        height:364px;
        margin: 9.5px 0px 0px -9px;
   }
    

    .personcontent {
      width: 256px;
      height: 539px;
      background: #FFFFFF;
      border-radius: 5px;
      margin-left: 10.8px;
      margin-top: 10px;
      position: relative;
      z-index: 9;
      min-width:256px;
      .usertop {
        display: flex;
        align-items: center;
        margin-top: 22px;
        margin-left: 28px;
      }
      .userbtn {
        display: flex;
        justify-content: center;
        align-items: center;
        margin-top: 26px;

        .gologin {
          width: 96px;
          height: 36px;
          background: #2a65ef;
          border-radius: 18px;
          line-height: 36px;
          text-align: center;
          margin-right: 18px;
          color: #ffffff;
          font-size: 14px;
          cursor: pointer;
        }

        .goregister {
          width: 97px;
          height: 37px;
          background: #f5f5f5;
          border: 1px solid #d8d8d8;
          border-radius: 18px;
          line-height: 37px;
          text-align: center;
          color: #666666;
          font-size: 14px;
          background: #ffffff;
          cursor: pointer;
        }
      }

      .userindustry {
        display: flex;
        justify-content: space-around;
        align-items: center;
        margin-top: 15px;

        .userindustrytit {
          color: #323232;
          font-size: 16px;
          font-weight: bold;
          position: relative;
          left: -30px;
        }

        .userindustrymore {
          font-size: 12px;
          color: #9c9c9c;
          cursor: pointer;
        }
      }
      .industrycontent{
        display:flex;
        justify-content: space-around;
        position: relative;
        top: -6px;
        cursor: pointer;
      }
      .industrytitle {
        display: flex;
        justify-content: space-around;
        align-items: center;
        margin-top: 25px;
        position relative
        .activecolor {
          width: 16px;
          height: 2px;
          background: #36A6FE;
          opacity: 1;
          position absolute
          top: 20px;
          left 22.5px
        }
        .fontcolor{
              color: #36A6FE;
              opacity: 1;
        }
        span {
          width 68px
          margin-left 2
          font-size: 14px;
          color: #323232;
          opacity: 0.5;
          font-weight: 400;
          line-height: 14px;
          overflow: hidden;
          text-overflow: ellipsis;
          display: -webkit-box;
          -webkit-line-clamp: 1;
          -webkit-box-orient: vertical;
        }
      }


      .industrydetail {
        line-height: 30px;
        color: #323232;
        padding-left 14px 
        padding-right 16px
        font-size 12px
        display flex
        align-items: center;
        .over{
          margin-left:9px;
          cursor: pointer;
          overflow: hidden;
          text-overflow: ellipsis;
          white-space: nowrap;
          height: 100%;
        //   display: -webkit-box;
        //   -webkit-line-clamp: 1;
        //   -webkit-box-orient: vertical;
        }
        .one{
           font-size 14px
           font-weight 400
           color #F04238
        }
        .two{
           font-size 14px
           font-weight 400
           color #F17738
        }
        .three{
           font-size 14px
           font-weight 400
           color #F6AF1E
         } 
         .other{
           font-size: 14px;
           font-weight 400
           color: #9C9C9C;
         }
      }
    }
  }
}


