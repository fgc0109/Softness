}
.html-active{
    color: #ffffff;
    background: #36a6fe;
    position: relative;
}
.trangle {
    content: "";
    width: 0px;
    height: 0px;
    border-top: 10px solid #36a6fe;
    border-left: 10px solid transparent;
    border-right: 10px solid transparent;
    position: absolute;
    top: 40px;
    left: calc(100% / 2);
}
.nav-flex{
    display flex
}
.html-content-foo{
    margin:16px;
    >>> img {
        max-width: 100%;
        height: auto !important;
    }
    >>> iframe {
    max-width: 100%;
    height: 300px;
    }
    >>> video {
        margin-top:15px;
        width:100%;
        height: 300px;
    }
    .tagname{
        font-size 16px
        color #676060
        margin-bottom 12px
    }
}

.zhichantree{
    background: #f5f7fc;
    border-radius: 5px;
    margin-right: 32px;
    max-height: 950px;
    font-size: 16px;
}
.treeItem{
    width: 218px;
    font-size: 16px;
}
.treechild{
    margin 18px 0
    text-align center
    cursor pointer
    font-size 16px
}
.treeactive{
    color: #36a6fe;
    font-size: 16px;

}
.details{
