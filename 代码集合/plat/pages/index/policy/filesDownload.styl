.frame_box{
    width: 100%;
    min-width: 1200px;
    background: #F1F1F1;
    font-size: 14px;
    font-family: Microsoft YaHei UI;
    .hei {
        height: 36px !important;
    }
    .bread_box {
        width: 1200px;
        padding-top: 20px;
        margin: 0 auto;
        font-size: 14px;
    }
    .sorting{
        width: 845px;
        margin: 10px auto;
        display: flex;
        justify-content: flex-end;
        padding-right: 20px;
        .sort_box {
            z-index: 9999;
            .sort_txt {
                display: inline-block;
                padding-left: 10px;
            }
        }
        .newestSort{
            color: #8A8A8A;
            cursor: pointer;
        }
        .hotestSort{
            color: #8A8A8A;
            cursor: pointer;
        }
        .activeSort{
            color: #36A6FE !important;
        }
        .inactiveSort{
            color: none !important;
        }
    }
    .file_box{
        width: 1200px;
        margin: 20px auto;
        min-height: 500px;
        display: flex;
        .file_list{
            width: 845px;
            min-height: 300px;
            background: #FFF;
            padding-bottom: 8px;
            .file_content {
                margin: 30px 0 20px 78px;
                display: grid;
                grid-template-columns: repeat(5, 96px);
                grid-template-rows: repeat(4, 172px);
                grid-gap: 16px 56px;
                .item {
                    .img_box {
                        position: relative;
                        .img_sty {
                            width: 100%;
                            height: 135px;
                            border-radius: 5px;
                            border: 1px solid #eaebef;
                            cursor: pointer;
                        }
                        .icon_type {
                            position: absolute;
                            right: 0;
                            bottom: 0;
                        }
                        .download_icon {
                            display: inline-block;
                            position: absolute;
                            right: 4px;
                            top: 0;
                            height: 16px;
                            font-size: 12px;
                            font-family: Microsoft YaHei, Microsoft YaHei-Regular;
                            font-weight: 400;
                            text-align: center;
                            color: #36a6fe;
                            line-height: 18px;
                            letter-spacing: 0.72px;
                            cursor: pointer;
                            .download_img {
                                width: 16px;
                                height: 16px;
                                vertical-align: middle;
                            }
                        }
                        .icon_buy {
                            display: inline-block;
                            position: absolute;
                            left: 4px;
                            bottom: 0;
                            width: 37px;
                            height: 15px;
                            background: #FF6D27;
                            border-radius: 8px;
                            font-size: 12px;
                            font-family: Microsoft YaHei, Microsoft YaHei-Light;
                            font-weight: 300;
                            color: #fcfcfc;
                            text-align: center;
                            letter-spacing: 0.72px;
                            cursor: pointer;
                        }
                    }
                    .title {
                        width: 88px;
                        height: 34px;
                        font-size: 12px;
                        font-family: Microsoft YaHei, Microsoft YaHei-Regular;
                        font-weight: 400;
                        color: #555555;
                        letter-spacing: 0.72px;
                        text-overflow: -o-ellipsis-lastline;
                        overflow: hidden;
                        text-overflow: ellipsis;
                        display: -webkit-box;
                        -webkit-line-clamp: 2;
                        line-clamp: 2;
                        -webkit-box-orient: vertical;
                        margin-bottom: 8px;
                        cursor: pointer;
                    }
                }
            }
            .search_frame{
                width: 100%;
                display: flex;
                align-items: center;
                min-height: 98px;
                position: relative;
                border-bottom: 1px solid #eaebef;
                .search img{
                    width: 18px;
                    height: 18px;
                    position: absolute;
                    left: 50px;
                    top: 42px;
                }
                .search_btn {
                    width: 115px;
                    height: 48px;
                    background: #36a6fe;
                    border-radius: 24px;
                    font-size: 16px;
                    font-family: Microsoft YaHei, Microsoft YaHei-Regular;
                    font-weight: 400;
                    color: #ffffff;
                    letter-spacing: 0.96px;
                    display: flex;
                    text-align: center;
                    align-items: center;
                    justify-content: center;
                    cursor: pointer;
                }
                .search_input{
                    width: 575px;
                    height: 48px;
                    background: #ffffff;
                    border: 2px solid #36a6fe;
                    border-radius: 26px;
                    padding-left: 38px;
                    margin: 27px 18px 23px 32px;
                }
                input:focus{
                    outline: 0;
                    padding-left: 38px;
                }
            }
            .no_data {
                height: 210px;
                width: 210px;
                text-align: center;
                margin: 50px auto;
                color: #BFBFBF;
                img {
                    width: 100%;
                    height: 100%;
                }
            }
        }
        .newSty{
            //background: #F9F9F9;
        }
        .right-box{
            width: 340px;
            margin-left: 10px;
            background: #FFF;
        }
    }
    .page_box {
        position: relative;
        padding: 30px 0;
        margin-top: 15px;
        .page_sty {
           position: absolute;
           left: 41%; 
           top: 50%; 
           transform: translate(-50%,-50%);
        }
    }
    .STY{
        color: #FFF !important;
        background: #F68234 !important;
    }
}