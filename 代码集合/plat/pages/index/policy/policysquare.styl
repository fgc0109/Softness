.iframe {
  width: 100%;
  min-width: 1200px;
  background: #F1F1F1;
  font-family: Microsoft YaHei UI;
  display: flex;
  flex-direction: column;
  .banner_img {
    width: 100%;
    height: 440px;
  }
  .banner_img>img{
    width: 100%;
    height: 440px;
  }
  .bread_sty {
    width: 100%;
    display: flex;
    font-size: 14px;
    font-family: Microsoft YaHei UI, Microsoft YaHei UI-Regular;
    font-weight: 400;
    color: #333333;
    letter-spacing: 0.84px;
    padding: 19px 0 16px 350px;
    background: #eff2f3;
  }
  .pad_a {
    padding: 55px 0 56.5px 366px;
  }
  .pad_b {
    padding: 44px 0 47px 366px;
  }
  .question {
    display: flex;
    flex-direction: column;
    width: 100%;
    background: #fff;
    .question_title {
      min-width: 1000px;
      display: flex;
      flex-direction: row;
      align-items: center;
      margin-bottom: 20px;
      .title {
        display: inline-block;
        height: 29px;
        font-size: 22px;
        font-family: Microsoft YaHei, Microsoft YaHei-Bold;
        font-weight: 700;
        color: #333333;
        letter-spacing: 1.32px;
      }
      .text {
        display: inline-block;
        height: 21px;
        font-size: 16px;
        font-family: Microsoft YaHei, Microsoft YaHei-Light;
        font-weight: 300;
        color: #36a6fe;
        letter-spacing: 0.96px;
        padding-left: 12px;
      }
      .article {
        font-size: 16px;
        font-family: Microsoft YaHei, Microsoft YaHei-Light;
        font-weight: 300;
        color: #7b7b7b;
        line-height: 21px;
        letter-spacing: 0.96px;
        padding-left: 12px;
      }
      .more {
        cursor: pointer;
        display: inline-block;
        width: 550px;
        height: 20px;
        font-size: 16px;
        font-family: Microsoft YaHei, Microsoft YaHei-Light;
        font-weight: 300;
        color: #b4b4b4;
        line-height: 21px;
        letter-spacing: 0.96px;
        text-align: right;
      }
      .hot {
        padding-left: 24px;
        width: 162px;
        height: 29px;
        font-size: 22px;
        font-family: Microsoft YaHei, Microsoft YaHei-Bold;
        font-weight: 700;
        text-align: left;
        color: #333333;
        line-height: 21px;
        letter-spacing: 1.32px;
      }
    }
    .question_con {
      display: flex;
      flex-direction: row;
      .question_left {
        .question_search {
          display: flex;
          align-items: center;
          position: relative;
          .question_icon {
            position: absolute;
            left: 18px;
            top: 15px;
            width: 18px;
            height: 18px;
          }
          .question_input>>>.el-input__inner {
            width: 600px;
            height: 48px;
            background: #ffffff;
            border: 2px solid #36a6fe;
            border-radius: 26px;
            padding-left: 40px;
          }
          .question_btn {
            cursor: pointer;
            width: 115px;
            height: 48px;
            background: #36a6fe;
            border-radius: 24px;
            margin-left: 8px;
            font-size: 16px;
            font-family: Microsoft YaHei, Microsoft YaHei-Regular;
            font-weight: 400;
            text-align: center;
            color: #ffffff;
            line-height: 48px;
            letter-spacing: 0.96px;
          }
        }
        .question_content {
          padding-top: 16px;
          .question_little {
            width: 100%;
            display: flex;
            flex-direction: row;
            justify-content: space-between;
            align-items: center;
            img {
              width: 18px;
              height: 18px;
            }
            .title {
              display: inline-block;
              font-size: 16px;
              font-family: Microsoft YaHei, Microsoft YaHei-Bold;
              font-weight: 700;
              color: #333333;
              line-height: 21px;
              letter-spacing: 0.96px;
            }
            .more {
              cursor: pointer;
              font-size: 16px;
              font-family: Microsoft YaHei, Microsoft YaHei-Light;
              font-weight: 300;
              color: #b4b4b4;
              line-height: 21px;
              letter-spacing: 0.96px;
            }
          }
          .question_list {
            margin-top: 11px;
            border-top: 1px solid #eaebef;
          }
        }
      }
      .question_right {
        display: flex;
        flex-direction: column;
        margin-left: 36px;
        height: 403px;
        .icon {
          width: 438px;
          height: 310px;
          margin-bottom: 9px;
        }
        .ask {
          width: 456px;
          height: 84px;
          cursor: pointer;
          margin-left: -7px;
        }
      }
    }
    .notice_con {
      display: flex;
      flex-direction: column;
      .notice_img {
        width: 18px;
        height: 18px;
        margin-right: 6px;
      }
      .notice_title {
        font-size: 16px;
        font-family: Microsoft YaHei, Microsoft YaHei-Bold;
        font-weight: 700;
        color: #333333;
        line-height: 21px;
        letter-spacing: 0.96px;
      }
      .notice_box {
        display: flex;
        flex-direction: row;
        margin: 12px 0 0 -121px;
        .notice_logo {
          width: 109px;
          height: 230px;
          margin-top: 122px;
        }
        .notice_calendar {
          min-width: 335px;
          width: 335px;
          height: 356px;
          background: #ffffff;
          border-radius: 10px;
          box-shadow: 0px 3px 6px 0px rgba(0,0,0,0.16); 
        }
        .notice_calendar>>>.el-calendar-table .el-calendar-day{
          width: 60px;
          height: 40px;
        }
        .notice_list {
          width: 823px;
          height: 356px;
          background: #ffffff;
          border-radius: 10px;
          box-shadow: 0px 3px 6px 0px rgba(0,0,0,0.16);
          margin-left: 32px; 
          .notice_date {
             margin: 11px 21px 14px 22px;
             display: flex;
             flex-direction: row;
             justify-content: space-between;
             .date_box {
              display: flex;
              flex-direction: row;
             }
            .month {
              font-size: 12px;
              font-family: Microsoft YaHei, Microsoft YaHei-Regular;
              font-weight: 400;
              color: #b4b4b4;
              line-height: 21px;
              letter-spacing: 0.72px;
              padding: 0 12px 0 18px;
              span:nth-child(1) {
                font-size: 19px;
                font-family: Microsoft YaHei, Microsoft YaHei-Regular;
                font-weight: 700;
                color: #36A6FE;
                line-height: 21px;
                letter-spacing: 0.72px;
              }
              span:nth-child(2){
                font-size: 18px;
                font-family: Microsoft YaHei, Microsoft YaHei-Regular;
                font-weight: 400;
                color: #333333;
                line-height: 21px;
                letter-spacing: 0.72px;
              }
            }
            .date {
              width: 184px;
              height: 24px;
              background: #f6fbff;
              border-radius: 4px;
              span:nth-child(1) {
                font-size: 12px;
                font-family: Microsoft YaHei, Microsoft YaHei-Regular;
                font-weight: 400;
                color: #b4b4b4;
                line-height: 21px;
                letter-spacing: 0.72px;
              }
              span:nth-child(2){
                font-size: 12px;
                font-family: Microsoft YaHei, Microsoft YaHei-Regular;
                font-weight: 400;
                color: #36a6fe;
                line-height: 21px;
                letter-spacing: 0.72px;
              }
            }
            .more{
              cursor: pointer;
              font-size: 16px;
              font-family: Microsoft YaHei, Microsoft YaHei-Light;
              font-weight: 300;
              color: #b4b4b4;
              line-height: 21px;
              letter-spacing: 0.96px;
            }
          }
          .notice_content {
            .notice_boxs {
              position: relative;
              .notice_icon {
                position: absolute;
                top: 280px;
                width: 100%;
                //margin: 5px 0 12px 0;
                text-align: center;
                img:nth-child(1) {
                  cursor: pointer;
                  width: 21px;
                  height: 20px;
                  margin-right: 24px;
                }
                img:nth-child(2) {
                  cursor: pointer;
                  width: 20px;
                  height: 20px;
                }
              }
            }
          }
        }
      }
    }
    .industry_con {
      display: flex;
      flex-direction: row;
      margin-left: -120px;
      .industry_left {
        width: 108px;
        height: 410px;
        img {
          width: 100%;
          height: 100%;
        }
      }
      .industry_tabs {
        width: 720px;
        margin: 0 27px 0 13px;
        display: flex;
        flex-direction: column;
        .industry_tab {
          display: flex;
          flex-direction: row;
          .tab_name {
            width: 167px;
            height: 21px;
            font-size: 16px;
            font-family: Microsoft YaHei, Microsoft YaHei-Regular;
            font-weight: 400;
            color: #555555;
            line-height: 21px;
            letter-spacing: 0.96px;
            margin-right: 37px;
            cursor: pointer;
          }
          .active {
            font-weight: 700;
            color: #333333;
          }
          .active:after {
            content: '';
            display: block;
            width: 30px;
            height: 4px;
            background: #36a6fe;
            border-radius: 2px;
            margin: 4px 0 0 19px;
          }
        }
        .industry_list {
          padding-top: 5px;
        } 
      }
      .industry_right {
        display: flex;
        flex-direction: column;
        .industry_title {
          display: flex;
          flex-direction: row;
          justify-content: space-between;
          margin-bottom: 14px;
          .right_title {
            height: 24px;
            font-size: 18px;
            font-family: Microsoft YaHei, Microsoft YaHei-Bold;
            font-weight: 700;
            color: #333333;
            line-height: 21px;
            letter-spacing: 1.08px;
          }
          .right_more {
            cursor: pointer;
            font-size: 16px;
            font-family: Microsoft YaHei, Microsoft YaHei-Light;
            font-weight: 300;
            color: #b4b4b4;
            line-height: 21px;
            letter-spacing: 0.96px;
          }
        }
      }
    }
    .document_con {
      display: flex;
      flex-direction: row;
      .document_list {
        width: 720px;
        min-width: 720px;
        display: flex;
        flex-wrap: wrap;
        justify-content: flex-start;
        .no-data {
          font-size: 16px;
          font-family: Microsoft YaHei, Microsoft YaHei-Bold;
          text-align: center;
          color: #b4b4b4;
          margin-top: 100px;
          width: 100%;
        }
        .item {
          width: 96px;
          min-width: 96px;
          border-radius: 5px;
          margin-right: 60px;
          .img_box {
            position: relative;
            width: 100%;
            height: 135px;
            border-radius: 5px;
            .img_sty {
              width: 100%;
              height: 100%;
              border: 1px solid #eaebef;
              border-radius: 5px;
              cursor: pointer;
            }
            .icon_type {
              position: absolute;
              right: 0;
              bottom: 0;
              width: 20px;
              height: 20px;
            }
            .icon_buy {
              display: inline-block;
              position: absolute;
              left: 4px;
              bottom: 0;
              width: 37px;
              height: 15px;
              background: #FF6D27;
              border-radius: 8px;
              font-size: 12px;
              font-family: Microsoft YaHei, Microsoft YaHei-Light;
              font-weight: 300;
              color: #fcfcfc;
              text-align: center;
              letter-spacing: 0.72px;
              cursor: pointer;
            }
            .download_icon {
              display: inline-block;
              position: absolute;
              right: 4px;
              top: 0;
              height: 16px;
              font-size: 12px;
              font-family: Microsoft YaHei, Microsoft YaHei-Regular;
              font-weight: 400;
              text-align: center;
              color: #36a6fe;
              line-height: 18px;
              letter-spacing: 0.72px;
              cursor: pointer;
              .download_img {
                width: 16px;
                height: 16px;
                vertical-align: middle;
              }
            }
          }
          .tit {
            width: 88px;
            height: 34px;
            display: inline-block;
            font-size: 12px;
            font-family: Microsoft YaHei, Microsoft YaHei-Regular;
            font-weight: 400;
            color: #333333;
            letter-spacing: 0.72px;
            text-overflow: -o-ellipsis-lastline;
            overflow: hidden;
            text-overflow: ellipsis;
            display: -webkit-box;
            -webkit-line-clamp: 2;
            line-clamp: 2;
            -webkit-box-orient: vertical;
            cursor: pointer;
          }
        }
        .item:nth-child(5n){
          margin-right: 0;
        }
      }
      .document_right {
        padding-left: 28px;
        .no-data {
          font-size: 16px;
          font-family: Microsoft YaHei, Microsoft YaHei-Bold;
          text-align: center;
          color: #b4b4b4;
          margin-top: 70px;
          width: 100%;
          display: flex;
          padding-left: 150px;
        }
      } 
    }
  }
  .nodata {
    font-size: 16px;
    font-family: Microsoft YaHei, Microsoft YaHei-Bold;
    text-align: center;
    color: #b4b4b4;
    margin-top: 120px;
    width: 100%;
  }
  .match_box {
    width: 100%;
    height: 280px;
    display: flex;
    background: #ffffff;
    background-size:100% 100%;
    background-image: url('~assets/images/zhengce/pic_matching.png')
    div {
      margin: auto;
      div:nth-child(1){
        width: 340px;
        height: 47px;
        font-size: 36px;
        font-family: Microsoft YaHei, Microsoft YaHei-Bold;
        font-weight: 700;
        text-align: center;
        color: #ffffff;
        line-height: 37px;
        letter-spacing: 2.16px;
        text-shadow: 0px 3px 6px 0px #0f2e9a; 
      }
      div:nth-child(2){
        margin-top: 6px;
        font-size: 17px;
        font-family: Microsoft YaHei, Microsoft YaHei-Regular;
        font-weight: 400;
        text-align: center;
        color: #cdd6f5;
        line-height: 18px;
        letter-spacing: 1.02px; 
      }
    }
  }
}