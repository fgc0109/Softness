flash() 
    animation:mymove 1.5s infinite;
    -webkit-animation:mymove 1.5s infinite; /*Safari and Chrome*/
    animation-direction:alternate;/*轮流反向播放动画。*/
    animation-timing-function: ease-in-out; /*动画的速度曲线*/
    /* Safari 和 Chrome */
    -webkit-animation:mymove 1.5s infinite;
    -webkit-animation-direction:alternate;/*轮流反向播放动画。*/
    -webkit-animation-timing-function: ease-in-out; /*动画的速度曲线*/
    @keyframes mymove
    {
        0%{
        transform: scale(1);  /*开始为原始大小*/
        }
        25%{
            transform: scale(1.2); /*放大1.1倍*/
        }
        50%{
            transform: scale(1);
        }
        75%{
            transform: scale(1.2);
        }

    }
    
    @-webkit-keyframes mymove /*Safari and Chrome*/
    {
        0%{
        transform: scale(1);  /*开始为原始大小*/
        }
        25%{
            transform: scale(1.2); /*放大1.1倍*/
        }
        50%{
            transform: scale(1);
        }
        75%{
            transform: scale(1.2);
        }
    }
.frame{
    width:100%;
    min-width:1200px;
    .banner{
        min-width:1200px;
        height: 440px;
        background-image: url('~assets/images/service/banner@2x2.png');
        background-size:100% 100%;
        flex-direction: column;
        font-weight: 700;
        color: #ffffff;
        >p:first-child{
            font-size: 70px;
            text-shadow: 0px 3px 2px 0px rgba(4,19,42,0.20); 
            letter-spacing: 1px;
            position relative
            .ask-now img{
                position absolute
                width: 86px
                height: 32px
                top 0
                right -100px
                cursor pointer
                flash()
            }
        }
        >p:nth-child(2){
            margin-top:20px;
            font-size: 24px;            
            letter-spacing: 22px;
            text-shadow: 0px 3px 2px 0px rgba(4,19,42,0.20); 
        }
        >p:nth-child(3){
            margin-top:26px;
            font-size: 24px;
            padding:6px 20px;
            color: #f5c158;
            letter-spacing: 2px;
            border-bottom: 2px solid rgba(245,193,88,0.40);
            text-shadow: 0px 3px 2px 0px rgba(4,19,42,0.20);  
        }
    }
    .why-wt{
        width:100%;
        height:538px;
        background-image: url('~assets/images/service/bg_1@2x.png');
        background-size:100% 100%;  
    }
    .brand{
        width:100%;
        height:520px;
        background-image: url('~assets/images/service/big_bg@2x.png');
        background-size:100% 100%;
    }
    .foot{
        width:100%;
        height:290px;
        background-image: url('~assets/images/service/bg_3@2x.png');
        background-size:100% 100%;
        .foot-list{
            width: 356px;
            height: 110px;
            background: #f1f9ff;
            border-radius: 10px;
            color:#666;
            font-size:16px;
            img{
                width:58px;
                height:58px;
                margin-right:20px
            }
            p:first-child{
                font-size:20px;
                color:#333;
                font-weight:700;
                margin-bottom:4px
            }
        }
    }
    .form-banner{
        height:232px;
        color:#fff;
        flex-direction: column;
        background-image: url('~assets/images/service/bg_4@2x.png');
        background-size:100% 100%;
        >p:first-child{
            font-size: 20px;
            font-weight:700;
        }
        >p:nth-child(2){
            font-size:14px;
            margin:4px 0 20px 0;
        }
        >div{
            >>> .el-input{
                width:774px;
            }
            >>> .el-input__inner {
                border-radius: 4px;
                height: 46px;
                line-height: 46px;
            }
            .form-btn{
                width: 180px;
                height: 46px;
                line-height: 46px;
                text-align: center;
                background: #fed91f;
                border-radius: 4px;
                font-weight: 700;
                color:#333;
                font-size:18px;
                margin-left:18px
                cursor pointer   
                webkit-transition: all .5s;
                transition: all .5s;
            }
            .form-btn:hover{
                box-shadow: 0px 0px 40px #32298c;
            }
        }
        >div+p{
            width:972px;
            margin-top:10px;
        }
         
    }
    .content{
        width:1190px;
        margin:42px auto 42px;
        display: flex;
        flex-direction: column;
        align-items: center;
        >p:first-child{
            font-size: 30px;
            font-weight: 700;
            color: #333;
            letter-spacing: 2px;
        }        
        .item{
            cursor pointer
            width: 282px;
            height: 368px;
            border-radius: 4px;
            padding:18px;
            display: flex;
            flex-direction: column;
            justify-content: space-between;
            align-items: center;
            border: 2px solid rgba(206,206,206,.6);            
            webkit-transition: all .5s;
            transition: all .5s;
            // box-shadow: 0px 5px 20px #ebebeb;
            cursor pointer
            img, .video-box video{
                width: 246px;
                height: 176px;
                webkit-transition: all .5s;
                transition: all .5s;
            }
            p{                
                text-overflow: -o-ellipsis-lastline;
                overflow: hidden;
                text-overflow: ellipsis;
                display: -webkit-box;
                -webkit-line-clamp: 2;
                line-clamp: 2;
                -webkit-box-orient: vertical;
            }
            .gs-list-title{
                font-size: 18px;
                font-weight: 700;
                color: #333;
            }
            .gs-list-desc{
                font-size: 14px;
                color: #666;
            }
            div:last-child{
                width: 254px;
                height: 34px;
                line-height:34px;
                background: #eff8ff;
                border-radius: 4px;
                text-align: center;
                color: #2a65ef;
                font-size:14px;
                img{
                    width: 14px;
                    height: 14px;
                    margin-left:8px;
                }
            }
        }
        .item:hover{
            border: 2px solid #2a65ef;
            margin-top: -30px
            box-shadow: 0px 3px 10px 0px rgba(0,0,0,0.12);  
            webkit-transform: translateZ(6px);
            transform: translateZ(6px);
            div:last-child{
                background: #2a65ef;
                color: #fff;
            }
            img, .video-box video{
                webkit-transform: scale(1.1);
                transform: scale(1.1);
            }
        }
        .team-item{
            width: 312px;
            border-radius: 4px;
            padding:16px;
            display: flex;
            flex-direction: column;
            justify-content: space-between;
            align-items: center;
            border: 2px solid rgba(206,206,206,.6);
            color:#333;
            font-size:16px;
            webkit-transition: all .5s;
            transition: all .5s;
            img{
                width: 100%;
                height: 270px;
            }
            p:nth-child(2){
                font-size: 16px;
                color: #333;
                margin:14px 0;
                span:first-child{
                    font-size:20px;
                    font-weight:700;
                    margin-right:14px
                }
            }
            p:nth-child(3){
                font-size: 14px;
                width:100%;
                padding:12px;
                background:#f9f9f9;
                color: #666;
                width:100%;
            }
            div:last-child{
                width: 100%;
                height: 34px;
                line-height:34px;
                background: #eff8ff;
                border-radius: 4px;
                text-align: center;
                webkit-transition: all .5s;
                transition: all .5s;
                color: #2a65ef;
                font-size:14px;
                margin-top:14px
                cursor pointer
                img{
                    width: 14px;
                    height: 14px;
                    margin-left:8px;
                }
            }
        }
        .team-item:hover{
            border: 2px solid #2a65ef;
            box-shadow: 0px 0px 10px 0px rgba(0,0,0,0.12); 
            webkit-transform: translateX(6px);
            transform: translateX(6px);
            div:last-child{
                background: #2a65ef;
                color: #fff;
            }
        }
    }
    .first{
        font-weight: bold;
        font-size: 16px;
        color: #000000;
        padding: 0 30px;
    }
    .two{
        font-size: 16px;
        color: #000000;
        padding: 15px 30px 40px 30px;
        padding-top: 15px
        line-height: 27px;
        font-weight: 400;
    }
    .ttt{
        width: 283px;
        height: 146px;
        position: relative;
        left: 50%;
        transform: translateX(-50%);
    }
}
