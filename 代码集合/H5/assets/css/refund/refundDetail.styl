.mutice_header_title{
    background: #fff !important;
    color: #333 !important;
    height: 1.1733333333rem;
}
.content{
    width: 100%;
    padding-top: 0.32rem;
}
.content_head{
    background: #ffffff;
    border-radius: 0.26666666666666666rem;
    position:relative;
    margin: 0 0.32rem 0 0.32rem;
}
.content_info{
    background: #ffffff;
    border-radius: 0.26666666666666666rem;
    margin: 0.32rem;
}
.content_title{
    font-size: 0.48rem;
    font-family: Source Han Sans CN, Source Han Sans CN-Bold;
    font-weight: 700;
    text-align: center;
    color: #333333;
    padding-top: 0.4266666666666667rem;
}
.content_time{
    font-size: 0.37333333333333335rem;
    font-family: Source Han Sans CN, Source Han Sans CN-Regular;
    font-weight: 400;
    text-align: left;
    color: #333333;
    margin: 0.4266666666666667rem 0.32rem 0;
}
.content_tip{
    font-size: 0.32rem;
    font-family: Source Han Sans CN, Source Han Sans CN-Regular;
    font-weight: 400;
