    padding-right: 0.16rem;
}
.filterSucess{
    margin-left: 0.4266666666666667rem
    color: #fd651c;
}
.flex{
    display: flex;
    justify-content: space-between;
    padding: 0 0 0.213333333333333rem 0;
    position relative
}
.type_flex{
    text-align: right;
}
.padd-top{
    text-align: right
    padding-bottom: .2133333rem
}
.wait_head{
