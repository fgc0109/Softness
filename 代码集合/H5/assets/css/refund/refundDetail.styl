    text-align: left;
    color: #333333;
    line-height: 0.42rem;
    padding-top: 0.32rem;
    margin-left: 0.32rem;
}
.infoSum_head{
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}
.info_sum{
    display: grid;
    grid-template-columns: 30% 52% 18%;
    margin:0.32rem
}
.info_detail{
    margin: 0.32rem;
    font-size: 0.37333333333333335rem;
    font-family: Source Han Sans CN, Source Han Sans CN-Regular;
    font-weight: 400;
    color: #333333;
    line-height: 0.42rem;
}
.infoSum_title{
    font-size: 0.37333333333333335rem;
    font-family: Source Han Sans CN, Source Han Sans CN-Medium;
    color: #333333;
    line-height: 17px;
    margin-left: 0.21333333333333335rem;
}
.info_type{
    font-size: 0.32rem;
    font-family: Source Han Sans CN, Source Han Sans CN-Regular;
    text-align: left;
    color: #999999;
    margin-top: 0.21333333333333335rem;
}
.info_price{
    font-size: 0.32rem;
    font-family: Source Han Sans CN, Source Han Sans CN-Regular;
    font-weight: 400;
    text-align: right;
    color: #999999;
}
.end{
    text-align: right;
}
.info_img img{
    width: 2.56rem   
