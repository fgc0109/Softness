}
.wait_time{
    font-size: 0.32rem;
    font-family: DINPro, DINPro-Regular;
    font-weight: 400;
    color: #999999;
    line-height: 0.32rem;
}
.wait_slogon{
    font-size: 0.32rem;
    font-family: Source Han Sans CN, Source Han Sans CN-Regular;
