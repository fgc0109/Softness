    height: 0.5333333333333333rem;
    text-align: center;    
    line-height: 0.52rem;
}
.bottom{
    position: fixed;
    bottom: 0;
    background: #36a6fe;
    width: 100%;
    height: 1.7066666666666668rem; 
    display:flex;   
    justify-content: flex-end;
}
.connect{
    width: 2.986666666666667rem;
    height: 1.0666666666666667rem;
    border: 0.02666666666666667rem solid #ffffff;
