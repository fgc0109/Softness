    text-align: center;
    line-height: 1.0133333333333334rem;
    color: #36a6fe;
    margin-right: 0.32rem;
}

.refund-dialog{
    width 7.466666666666667rem
    background: #ffffff;
    border-radius: 0.32rem;
    padding-top 0.4266666666666667rem
    overflow hidden
    .title{
        font-weight: 500;
        font-size: 0.48rem;
        text-align center
        color: #000000;
        margin-bottom 0.32rem
    }
    .cancel_content{
        font-size: 0.37333333333333335rem;
        text-align center
        color: #000000;
    }
    .heng{
        width 100%
        height: 0.02666666666666667rem;
        border: 0.013333333333333334rem solid #e4e4e463;
        margin-top 0.32rem
    }
    .btns{
        display flex
        font-weight: 500;
        font-size: 0.48rem;
        .cancel{
          color: #999999;
          flex 1
          text-align center
          border-right: 0.013333333333333334rem solid #e4e4e4;
          height 1.3333333333333333rem
          line-height 1.0666666666666667rem
        }
        .makesure{
          color: #36a6fe;
          flex 1
          text-align center
          height 1.3333333333333333rem
          line-height 1.0666666666666667rem
        }
    }
}
