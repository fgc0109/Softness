.order-frame
    min-height: 95vh
    margin-top: 40px

.tab-order
    width: 100%
    background: #fff
    position fixed
    top: 40px
    left: 0
    z-index 999
    padding: 0 .4rem
    border-top: 1px solid #F4F6F8
    box-shadow: 3px 3px 4px 0px rgba(0,0,0,0.03); 
    ul
        display: flex
        font-size: 0.37333333333rem
        .tab-item
            height: 1.333333333rem
            line-height: 1.3333333rem
            flex: 1
            text-align: center
            position relative
            .tab-block
                color: #333
                font-weight: 500;
                font-family:Source Han Sans CN
            
            .bottom-line
                position absolute
                width: 18px;
                height: 4px;
                background: #36a6fe;
                border-radius: 4px;
                left: 50%;
                top: 100%;
                transform: translate(-50%,-100%);
            
        
        .choosed-tab
            .tab-block
                color: #36A6FE
                        
        
    

.order-list
    width: 100%
    padding-top: 1.4rem
    .list-item
        margin .32rem
        background: #fff
        border-radius: 10px;
        font-family: Source Han Sans CN
        letter-spacing: 1px
        .time
            width: 100%
            display: grid;
            grid-template-columns: 65% 35%
            height: 1.06666667rem
            line-height: 1.06666667rem
            padding: 0 .32rem
            border-bottom: 1px solid #F4F4F4
            p
                flex: 1
                font-size: .37333333rem
                color: #333
                white-space nowrap
            
            .obligation
                text-align: right;
                color: #FF6D27
                font-size: .4266666667rem
                text-decoration: none
            .refundobligation
                text-align: right;
                font-size: .4266666667rem
                text-decoration: none
                font-weight: 500;
            span
                font-size .32rem
                color: #333333
        
        .list-body
            padding: .32rem
            display: flex
            align-items: center
            width: 100%
            position: relative
            .pro-img
                width: 31%
                height: 0
                padding-bottom: 22%
                overflow: hidden
                img
                    width: 100%
                
                video
                    width: 100%
                
            
            .item-info
                width: 48%
                margin-left: 2%
                font-family: Source Han Sans CN
                h3
                    font-size: .373333333rem
                    color: #333
                    text-overflow: -o-ellipsis-lastline;
                    overflow: hidden;
                    text-overflow: ellipsis;
                    display: -webkit-box;
                    -webkit-line-clamp: 2;
                    line-clamp: 2;
                    -webkit-box-orient: vertical;
                
                p
                    color: #999
                    font-size: .32rem
                
            
            .price-num
                width: 21%
                margin-left: 1%
                text-align: right
                color: #999
                .single-price
                    font-size: .32rem
                    font-weight: 500
                    span
                        font-size: .32rem
                    
                
                .count
                    font-size: .32rem
                    -webkit-transform: scale(0.8)
        .actual-pay
            padding-bottom: .45333333333rem
            margin-right: .32rem
            font-size: .373333333rem
            font-weight: 600
            display: flex
            justify-content: flex-end
            .price-desc, .price-desc span
                color: #999
                font-weight: 400   
            .price-desc
                margin-left .16rem
                font-size: .32rem    
        .padding-b10
            padding-bottom: .16rem 
        .list-handle
            display: flex
            border-top: 1px solid #F4F4F4
            align-items: center
            padding: 0 .32rem
            .transfer-txt
                p
                    text-align: left
                    font-size: .32rem
                    align-items: center
                .warning-img
                    width: .34666rem
                    height: .34666rem
            .handle-btn
                flex: 1
                text-align: right
                margin .26666666667rem 0
                span
                    width: 2.02666667rem
                    height: .9066666667rem
                    line-height: .9866666667rem
                    padding: .2rem .32rem
                    text-align: center
                    font-size: .32rem
                    border-radius: 34px 
                .bt-cancle
                    border: 1px solid #999999;
                    color: #333
            
                .bt-pay
                    background: #36a6fe;
                    color: #fff
                
                .pay-order-cancel
                    background: #FFFFFF
                    color: #999999
                    border: 1px solid #F4F4F4        
            
            
            
            
            
            
        
    

.page_limit 
    display: flex;
    justify-content: center;
    align-items: center;
    margin-top: 0.53333333333rem; 
    margin-bottom: .5rem;
    width: 90%

.null_search 
    color: #aaaaaa;
    font-size: 0.426666666667rem;
    display: flex;
    justify-content: center;
    align-items: center;
    margin-top: 0.8rem;

.empty
    height: 85vh
    margin-top: 20px
    background: #FFFFFF
    .empty-img
        height: 100px;
        width:100px;
        text-align: center;
        color: #666666
        background: #FFFFFF;
        margin: 0 auto
        padding-top: 35%
        img
            width: 100px
            height: 100px
        
    

.auth-box
    background #FFFFFF
    position fixed
    top 25%
    left 5%
    z-index 9999
    width 90%
    min-height 122px
    border-radius 5px
    color #333333
    overflow hidden
    padding: 10px 25px
    img
        width: .32rem
        height: .32rem
        position absolute
        right 12px
        top 12px
    

.del-box
    background #FFFFFF
    position fixed
    top 25%
    left 5%
    z-index 9999
    width 90%
    height 178px
    border-radius 5px
    color #333333
    overflow hidden
    img
        width: .32rem
        height: .32rem
        position absolute
        right 21px
        top 21px
    
    .del-title
        font-size 16px
        line-height 48px
        background #eef5f8
        text-align center
        position relative
    
    .del-content
        font-size: 16px
        padding 20px 30px
    
    .del-btn
        margin-left 15%
        text-align center
        display flex
        .cancel-del-btn
            width 102px
            height 35px
            margin-right 20px
            border-radius 3px
            font-size 16px
            line-height 35px
            text-align center
            color #FFFFFF
            background #36A6FE
            cursor pointer
        
        .confirm-del-btn
            color #333333
            background rgb(224,242,251)
// 退款
 .item-info-refund
    margin-left: 2%
    font-family: Source Han Sans CN
    h3
        font-size: .373333333rem
        color: #333
        text-overflow: -o-ellipsis-lastline;
        overflow: hidden;
        text-overflow: ellipsis;
        display: -webkit-box;
        -webkit-line-clamp: 2;
        line-clamp: 2;
        -webkit-box-orient: vertical;
        width: 4.08rem
    
    p
        color: #999
        font-size: .32rem 

.type{
    margin-top 0.08rem
}
.refund-money{
    color: #333333;
    font-size: 10px;
    position: absolute;
    right: 0.32rem;
    font-weight 550
}        
.cancelrefund{
    background #fff !important
    border: 1px solid #999999;
    color: #333333 !important;
    margin-right:0.26666666666666666rem;
}
.refund-dialog{
    width 7.466666666666667rem
    height 4.266666666666667rem
    background: #ffffff;
    border-radius: 0.32rem;
    padding-top 0.4266666666666667rem
    overflow hidden
    .title{
        font-weight: 500;
        font-size: 0.48rem;
        text-align center
        color: #000000;
        margin-bottom 0.32rem
    }
    .content{
        font-size: 0.37333333333333335rem;
        text-align center
        color: #000000;
    }
    .heng{
        width 100%
        height: 0.02666666666666667rem;
        border: 0.5px solid #e4e4e463;
        margin-top 0.32rem
    }
    .btns{
        display flex
        font-weight: 500;
        font-size: 18px;

        .cancel{
          color: #999999;
          flex 1
          text-align center
          border-right: 0.5px solid #e4e4e4;
          height 50px
          line-height 40px
        }
        .makesure{
          color: #36a6fe;
          flex 1
          text-align center
          height 50px
          line-height 40px
        }
    }
}
.mint-popup{
    background none
}

