/* pages/square.wxss */
page {
  background: #FFFFFF;
}
.square-wrap{
  width: 100%;
  font-size: 32rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Bold;
  font-weight: 700;
  color: #333333;
}
.square-nav{
  width: 100%;
  position: fixed;
  left: 0;
  display: flex;
  align-items: center;
  background: #fff;
  z-index: 999;
}
.item-lang {
  width: 30rpx;
  height: 8rpx;
  background-color: transparent;
}
.no-more{
  width: 100%;
  text-align: center;
  font-size: 28rpx;
  color: #999;
  background: #FFFFFF;
  padding: 100rpx 0 68rpx 0;
}
.square-item{
  width: 50%;
  height: 88rpx;
  line-height: 88rpx;
  position: relative;
  display: flex;
  justify-content: center;
}
.item-title{
  color: #999;
  font-size: 32rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
}
.item-title-active{
  font-weight: bold;
  color: #333;
}
.item-lang-active{
  width: 30rpx;
  height: 8rpx;
  background: #36a6fe;
  border-radius: 4rpx;
  position: absolute;
  bottom: 5rpx;
  left: 0;
  right: 30rpx;
  margin: 0 auto;
}
.info-wrap{
  width: 100%;
  font-size: 32rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Bold;
  font-weight: 700;
  color: #333333;
}
.question-button{
  position: fixed;
  color: #fff;
  background: #36A6FE;
  border-radius: 50%;
  z-index: 100000;
  width: 90rpx;
  height: 90rpx;
  text-align: center;
  line-height: 90rpx;
  bottom: 220rpx;
  right: 40rpx;
  font-size: 28rpx;
}
.info-nav{
  width: 100%;
  position: fixed;
  left: 0;
  top: 85rpx;
  display: flex;
  align-items: center;
  background: #eef1f2;
  z-index: 999;
  font-size: 28rpx;
}
.item-lang-child {
  width: 20rpx;
  height: 8rpx;
  background-color: transparent;
}
.no-more-child{
  width: 100%;
  text-align: center;
  font-size: 28rpx;
  color: #999;
  background: #F5F7FB;
  padding: 0 0 68rpx 0;
}
.filter-item-child{
  width: 50%;
  height: 88rpx;
  line-height: 88rpx;
  position: relative;
  display: flex;
  justify-content: center;
}
.item-title-child{
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  color: #666666;
  font-size: 28rpx;
  width: 64px;
}
.item-title-active-child{
  font-weight: bold;
  color: #555555;
}
.item-lang-active-child{
  width: 20rpx;
  height: 8rpx;
  background: #36a6fe;
  border-radius: 4rpx;
  position: absolute;
  bottom: 5rpx;
  left: 0;
  right: 30rpx;
  margin: 0 auto;
}

/* 课程 */
.class-box {
  padding-top: 90rpx;
  padding-bottom: 20rpx;
}

.class-room {
  padding-bottom: 120rpx;
}

/*
 轮播图+搜索框
*/
.class-bg {
  position: relative;
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  padding-top: 24rpx;
}

.class-search {
  position: absolute;
  top: 20rpx;
  left: 50%;
  transform: translateX(-50%);
  width: 628rpx;
  height: 60rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  background: #ffffff;
  border-radius: 30rpx;
  font-size: 24rpx;
  z-index: 999;
}

.class-search image {
  width: 30rpx;
  height: 30rpx;
  margin-left: 21rpx;
  margin-right: 12rpx;
}

.bg-img {
  width: 702rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  height: 358rpx;
}

.bg-img image {
  width: 702rpx;
  height: 100%;
}

/*
 短视频
*/
.short-video {
  width: 100%;
  margin-top: 56rpx;
}

.scroll-video {
  width: 100%;
  display: flex;
  padding-left: 24rpx;
  margin-top: 18rpx;
}

.scroll-video-view {
  width: 100%;
}

.scroll-video-item {
  position: relative;
  width: 208rpx;
  height: 370rpx;
  padding-right: 18rpx;
  padding-bottom: 18rpx;
}

.short-video image {
  width: 208rpx;
  height: 370rpx;
  background: rgba(0, 0, 0, 0.00);
  border-radius: 16rpx;
}

.short-video video {
  width: 208rpx;
  height: 370rpx;
  background: rgba(0, 0, 0, 0.00);
  border-radius: 16rpx;
}

.play-contain {
  height: 40rpx;
  position: absolute;
  bottom: 30rpx;
  right: 28rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #F5F7FB;
  font-size: 24rpx;
  background: rgba(0, 0, 0, 0.6);
  padding: 0rpx 16rpx;
  border-radius: 20rpx;
}

.play-contain>image {
  display: inline-block;
  width: 18rpx;
  height: 21rpx;
  border-radius: 0rpx;
  margin-right: 8rpx;
}


/*
 免费专区
*/
.free-zone {
  margin-top: 64rpx;
}

.free-list-contain {
  background: #EEF1F2;
  margin-top: 18rpx;
}

.free-list {
  width: 702rpx;
  background: #ffffff;
  display: flex;
  flex-wrap: wrap;
  padding: 24rpx 0 0 0;
  margin-left: 24rpx;
  border-radius: 20rpx;
}

.free-list-item {
  width: 300rpx;
  display: flex;
  flex-direction: column;
  background: #FAFCFF;
  font-size: 26rpx;
  border-bottom-left-radius: 16rpx;
  border-bottom-right-radius: 16rpx;
  padding-bottom: 16rpx;
  margin-bottom: 24rpx;
  margin-left: 38rpx;
}

.wrap-sty {
  margin-right: 0rpx;
}

.right-wrap-sty {
  margin-left: 26rpx;
}

.free-list image {
  width: 300rpx;
  height: 168rpx;
  background: rgba(0, 0, 0, 0.00);
  border-top-left-radius: 16rpx;
  border-top-right-radius: 16rpx;
}

.free-list-item>text:nth-of-type(1) {
  width: 304rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-weight: bold;
  margin: 7rpx 9rpx;
}

.free-list-item>text:nth-of-type(2) {
  width: 304rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-size: 24rpx;
  color: #CDCDCD;
  margin: 0rpx 9rpx;
}

.free-list-item>text:nth-of-type(3) {
  color: #FC8A2A;
  margin: 8rpx 9rpx;
}

/*
 VIP免费专区
*/
.vip-free-zone {
  margin-top: 58rpx;
}

.vip-free-main {
  width: 100%;
  /* height: 660rpx; */
  position: relative;
}

.vip-free-bg {
  width: 100%;
  height: 100%;
}

.vip-free-contain {
  background: linear-gradient(0deg, #fff 38%, #5a9ef7fb);
  padding: 2%;
  margin: 24rpx 24rpx 24rpx 30rpx;
  border-radius: 24rpx;
}

.vip-free-img {
  width: 660rpx;
  height: 280rpx;
  background: rgba(0, 0, 0, 0.00);
  border-radius: 20rpx 20rpx 0 0;
}

.vip-free-list {
  display: flex;
  flex-direction: column;
}

.vip-free-item {
  display: flex;
  align-items: center;
  height: 89rpx;
  border-bottom: 2rpx solid #E3E3E3;
}

.vip-free-item:last-of-type {
  border-bottom: none;
}

.vip-free-item>text:nth-of-type(1) {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 40rpx;
  height: 40rpx;
  background: #ebf6ff;
  border: 2rpx solid #0a76f1;
  border-radius: 8rpx;
  color: #0a76f1;
  font-size: 28rpx;
  margin-left: 12rpx;
}

.vip-free-item>text:nth-of-type(2) {
  display: inline-block;
  width: 610rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  color: #555555;
  font-size: 28rpx;
  margin-left: 19rpx;
}

/*
 分类列表
*/

.scroll-contain {
  width: 96%;
  white-space: nowrap;
}

.new-scroll {
  display: flex;
  padding: 0 24rpx;
}

.scroll-item {
  position: relative;
  margin-right: 40rpx;
  width: 134rpx;
  height: 80rpx;
  color: #999999;
  font-size: 32rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.scroll-item>text:nth-of-type(2) {
  position: absolute;
  bottom: 0;
  left: 50%;
  transform: translateX(-50%);
  display: inline-block;
  width: 42rpx;
  height: 10rpx;
  border-radius: 6rpx;
  background: #418AFE;
  margin-top: 18rpx;
}

.active-scroll-sty {
  color: #333333 !important;
  font-size: 32rpx !important;
  font-weight: bold;
}

.course-list {
  width: 94%;
  margin: 0 3%;
  margin-top: 16rpx;
  background: #ffffff;
  border-radius: 20rpx;
  padding-bottom: 24rpx;
}

.more-course {
  display: flex;
  width: 671rpx;
  margin-left: 50%;
  transform: translate(-50%);
  justify-content: center;
  align-items: center;
  border-top: 2rpx solid #E5E5E6;
  padding: 28rpx 28rpx 16rpx 28rpx;
  margin-top: 19rpx;
}

.more-course text {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 670rpx;
  height: 80rpx;
  background: #edf2fa;
  border-radius: 0px 0px 20rpx 20rpx;
  color: #418AFE;
  font-weight: bold;
  font-size: 28rpx;
}

.classify-img {
  width: 697rpx;
  height: 168rpx;
  margin-left: 50%;
  transform: translate(-50%);
  border-radius: 8rpx;
  margin-top: 43rpx;
}

/*
 精品课程
*/
.quality-courses {
  margin-top: 64rpx;
}

.quality-course-list {
  width: 94%;
  margin: 0 3%;
  margin-top: 16rpx;
  background: #ffffff;
  border-radius: 20rpx;
  padding-bottom: 40rpx;
}

/*
 公共样式
*/
.common-title {
  position: relative;
  display: flex;
  align-items: center;
  color: #333333;
  font-weight: bold;
  font-size: 32rpx;
}

.common-title>text:nth-of-type(1) {
  display: inline-block;
  width: 10rpx;
  height: 40rpx;
  background: linear-gradient(0deg, #33a7ff, #3a81fd);
  margin-right: 14rpx;
  margin-left: 24rpx;
}

.common-more {
  position: absolute;
  display: flex;
  align-items: center;
  top: 50%;
  right: 30rpx;
  transform: translateY(-50%);
  color: #BCBCBC;
  font-size: 26rpx;
  font-weight: normal;
}

.common-more>text:nth-of-type(2) {
  display: inline-block;
  width: 12rpx;
  height: 12rpx;
  border-top: 2rpx solid #BCBCBC;
  border-right: 2rpx solid #BCBCBC;
  transform: rotate(45deg);
  margin-top: 4rpx;
  margin-left: 2rpx;
}

/* 资讯列表 */
.view-courses {
  margin-top: 250rpx;
}

.view-course-list {
  width: 100%;
  min-height: 850rpx;
  background: #ffffff;
  padding-bottom: 140rpx;
  border-top: 2rpx solid #f7f5f5;
  overflow-x: hidden;
}

.view-file-list{
  width: 100%;
  min-height: 850rpx;
  background: #ffffff;
  padding: 20rpx 40rpx 140rpx;
  border-top: 2rpx solid #f7f5f5;
  overflow-x: hidden;
}

/* 搜索 */
.top-fixed {
  position: fixed;
  top: 168rpx;
  z-index: 9999;
  width: 100%;
  height: 120rpx;
  background: #FFFFFF;
  display: flex;
  justify-content: center;
  align-items: center;
}

.search-contain {
  display: flex;
  width: 600rpx;
  align-items: center;
  height: 72rpx;
  background: #ffffff;
  border-radius: 36rpx;
  border: 2rpx solid #36A6FE;
}

.search-contain image {
  width: 32rpx;
  height: 32rpx;
}

.search-contain-left {
  display: flex;
  justify-content: center;
  align-items: center;
}

.search-contain-left text {
  margin-left: 25rpx;
  margin-right: 4rpx;
  color: #333333;
  font-size: 26rpx;
  font-weight: bold;
}

.lang-sty {
  width: 2rpx;
  height: 40rpx;
  background: #e4e4e4;
  margin-left: 12rpx;
  margin-right: 20rpx;
}

.search-contain-right {
  display: flex;
  align-items: center;
}

.search-contain-right text {
  display: inline-block;
  color: #666666;
  font-size: 32rpx;
}

.fixed-search {
  color: #333333;
  font-weight: 700;
  font-size: 32rpx;
  margin-left: 24rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Medium;
}

.holder-style{
  color: #999;
  font-size: 28rpx;
}

.input-class{
  margin-left: 30rpx;
}

/* 筛选项 */
.filter-contain {
  width: 100%;
  white-space: nowrap;
  padding: 20rpx 0 16rpx 0;
  background: #ffffff;
  position: fixed;
  top: 270rpx;
  right: 0;
  z-index: 999;
  box-shadow: 0px 3px 3px rgba(51, 51, 51, 0.11);
}

.filter-content {
  width: 100%;
  background: #ffffff;
  padding-left: 30rpx;
  padding-bottom: 20rpx;
  position: relative;
}

.filter-scroll {
  display: flex;
}
.filter-item:first-child {
  margin-left: 24rpx;
}
.filter-item {
  height: 56rpx;
  background: #deefff;
  border-radius: 12rpx;
  color: #ffffff;
  font-size: 24rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  text-align: center;
  margin-right: 10rpx;
  z-index: 999;
}

.filter-item>text:nth-of-type(2) {
  display: inline-block;
  width: 0rpx;
  height: 0rpx;
  border-top: 12rpx solid transparent;
  border-right: 12rpx solid transparent;
  border-left: 12rpx solid #888888;
  transform: rotate(-45deg) translateY(-50%);
  margin-left: 8rpx;
}

.filter-scroll-sty {
  color: #747474;
}

.filter-angle-sty {
  border-left: 12rpx solid #747474 !important;
}

.option-list {
  width: 100%;
  position: absolute;
  top: 76rpx;
  left: 0;
  background: #ffffff;
  display: flex;
  flex-wrap: wrap;
  z-index: 999;
  padding: 20rpx 0 50rpx 30rpx;
}

.mask {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 76rpx;
  left: 0;
  background: rgba(0, 0, 0, 0.5);
  display: flex;
  flex-wrap: wrap;
  z-index: 998;
  padding: 20rpx 0 50rpx 30rpx;
}

.pad1 {
  padding: 20rpx 0 150rpx 30rpx;
}

.pad2 {
  padding: 20rpx 0 110rpx 30rpx;
}

.option-item {
  margin-right: 24rpx;
  margin-bottom: 24rpx;
  font-size: 24rpx;
  color: #888888;
}

.active-option {
  color: #36A6FE !important;
}

/*
  遮罩样式
*/
.global-tip {
  position: fixed;
  overflow-y: hidden;
  width: 100%;
  height: 100%;
  top: 390rpx;
  left: 0;
  background: rgba(0, 0, 0, 0.6);
  z-index: 998;
}

.no-data {
  margin-top: 50rpx;
  color: #666666;
  display: flex;
  justify-content: center;
}

.filter-list {
  position: fixed;
  width: 100%;
  max-height: 480rpx;
  overflow-y: scroll;
  top: 346rpx;
  left: 0;
  z-index: 999;
  background-color: #ffffff;
}

.filtrate-wrapper {
  padding: 15rpx;
  overflow-y: scroll;
}
 
.filtrate-title {
  margin-top: 25rpx;
  font-weight: bold;
  color: #000000;
  font-size: 16px;
}

.filtrate-item{
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}
.rank-item {
  /* width: 100rpx; */
  height: 50rpx;
  background: #F8F8F7;
  opacity: 1;
  border-radius: 10rpx;
  text-align: center;
  font-size: 24rpx;
  font-weight: 400;
  line-height: 50rpx;
  color: #333333;
  opacity: 1;
  margin-right: 12rpx;
  margin-top: 15rpx;
  padding: 5rpx 20rpx;
}

.chooseRank {
  color: #FFFFFF;
  background: #36A6FE;
}

.filtrate-btn {
  display: flex;
  flex-direction: row;
  width: 100%;
  height: 60rpx;
  color: #ffffff;
  justify-content: center;
  margin-top: 80rpx;
  margin-bottom: 40rpx;
  font-size: 26rpx;
  font-weight: 400;
  text-align: center;
}

.filtrate-reset {
  flex: 0.5;
  height: 66rpx;
  line-height: 66rpx;
  border-radius: 50rpx;
  color: #333333;
  border: 2rpx solid #999999;

}

.filtrate-submit {
  flex: 0.5;
  height: 70rpx;
  line-height: 70rpx;
  background: #36A6FE;
  border-radius: 50rpx;
  margin-left: 30rpx;
}

.flex-sty {
  display: flex;
  width: 100%;
}

.pad-sty {
  padding-top: 70rpx;
}