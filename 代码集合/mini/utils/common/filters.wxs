function filterLevel(val) {
  return val == 1 ? '国家级' : val == 2 ? '省级' : val == 3 ? '市级' : val == 4 ? '区级' : ''
}

function filterOption(val,num) {
  if(val){
    return val.length > num ? val.slice(0,num) + '...': val
  }
}

function filterElement(arr, str) {
  return arr.indexOf(str) > -1 ? true : false
}

function filterAmount(val) {
  if (val != undefined) {
    return val.toFixed(2)
  }
}

module.exports = {
  filterList: filterList,
  returnLength: returnLength,
  returnList: returnList,
  subFirstCode: subFirstCode,
  getProClassifyName: getProClassifyName,
  getProjectIcon: getProjectIcon,
  filterType: filterType,
  handleStr: handleStr,
  filterLevel: filterLevel,
  returnCate: returnCate,
  filterOption: filterOption,
  filterElement: filterElement,
  filterAmount: filterAmount
}