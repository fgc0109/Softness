function filterList(start, end, list) {
  if (list.length < 5) {
    if (end > 4) {
      return list.slice(start - 4, end - 4)
    } else {
      return list.slice(start, end)
    }
  } else if (list.length > 4 && list.length < 8) {
    return (list.slice(start, end)).concat(list.slice(0, 8 - list.length))
  } else {
    return list.slice(start, end)
  }
}

function returnList(start, end, list) {
  if(list){
    return list.slice(start, end)
  }
}

function returnCate(val) {
  if(val){
    return val == 1 ? '项目申报' : val == 2 ? '知识产权' : val == 7 ? '工商财税' : val == 9 ? '融资上市' : val == 10 ? '法律服务':val == 11 ? '软件开发':''
  }
}

function returnLength(list) {
  return list.length;
}

function handleStr(str) {
  var newstr;
  if(str == '香港' || str == '台湾' || str == '澳门'){
    newstr = str
  }else{
    newstr = str.lastIndexOf('市') ? str.slice(0,str.lastIndexOf('市')) : str
    if(newstr.length>3){
      newstr = newstr.slice(0, 3) + '...'
    }
  }
  return newstr;
}

