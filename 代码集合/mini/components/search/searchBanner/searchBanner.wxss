.bannerTop-global{
  width:750rpx;
  background:#fff;
}
.bannerTop{
  width:702rpx;
}
.banner-global{
  height:88rpx;
  display:flex;
  color:#86898D;
  font-size:26rpx;
  background:#fff;
  border-bottom:1px solid #D9D9D9;
  display: flex;
  align-items: center;
}
.banner{
  width:702rpx;
  background:#fff;
  display:flex;
  align-items: center;
  color:#86898D;
  font-size:26rpx;
  margin-left:24rpx;
  border-radius: 8rpx;
  position: relative;
  top:-60rpx;
  height:96rpx;
  box-shadow:0 0 3px rgba(148, 145, 145, 0.5);
}
.no-radius{
  border-radius: 8rpx 8rpx 0 0;
}
.banner-checked{
  color:#36A6FE
}
.banner-item-global{
  width:250rpx;
  border-right:2px solid #C4C4C4;
  display: flex;
  justify-content: center;
  align-items: center;
}
.banner-item{
  width:234rpx;
  height:40rpx;
  border-right:2px solid #C4C4C4;
  display: flex;
  justify-content: center;
  align-items: center;
}
.no-right{
  border-right:0;
}
.text{
  color:#C4C4C4;
  /* margin-left:80rpx */
  /* position: relative;
  left:80rpx */
}
.down{
  width: 0;
	height: 0;
	margin-left:16rpx;
	border-top: 10rpx solid #D9D9D9;
	border-right: 10rpx solid transparent;
	border-left:10rpx solid transparent;
}
.down-choosed{
  width: 0;
  height: 0;
	margin-left:16rpx;
	border-bottom: 10rpx solid #36A6FE;
	border-right: 10rpx solid transparent;
	border-left:10rpx solid transparent;
}
.dialog-global{
  width:100%;
  height: 896rpx;
  z-index: 999;
  background:#fff;
  /* border:1px solid red */
}
.dialog{
  width:702rpx;
  height: 896rpx;
  z-index: 999;
  background:#fff;
  margin-left:24rpx;
  padding:20rpx 0;
  margin-top:-62rpx
}
.show-list-item{
  color:#3A414B;
  font-size:26rpx;
  padding:27rpx 10rpx 27rpx 76rpx;
  border-bottom:1px solid #e9e9e9;  
  /* margin-left:81rpx; */
}
.children{
  color:#86898D;
  display: flex;
  flex-wrap: wrap;
