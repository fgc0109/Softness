.content{
  margin: 0 auto;
  margin-bottom: 60rpx;
  height: 80rpx;
  background: #ededed;
  color: #999;
  border-radius: 10rpx;
  display:flex;
  justify-content:center;
  align-items:center;
  font-size:28rpx;  
}
.box{
  /* width: 120rpx; */
  height: 80rpx;
  border-radius: 10rpx;
  display: flex;
  justify-content: center;
  box-sizing: border-box;
  align-items: center;
}
.primary-block{
  /* border: 1px solid rgba(0, 0, 0, 0.2); */
  /* border-radius: 10rpx 0 0 10rpx; */
  background-color: #757B8B;
}
.success-block{
  background-color: #07c160;
  /* border-radius: 0 10rpx 10rpx 0; */
  margin-top: -2rpx;
}
.success-bg{
  background-color: #F8F8F8;
  box-sizing: border-box;
  color: #07c160;
  font-weight: bold;
  border: 2rpx solid #07c160;
}
.movable-icon{
  width: 40rpx;
  height: 40rpx;
  background: url(data:image/png;base64,) no-repeat center center;
  background-size: 100% 100%;
}