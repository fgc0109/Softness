/* components/project/priceInfo/priceInfo.wxss */
.price-content {
  height: 106rpx;
  background: #EFF8FF;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  border-radius: 0 20rpx 20rpx 0;
}

.show-price {
  width: 416rpx;
  height: 106rpx;
  background: linear-gradient(126deg, #ff762d 7%, #ffb759 100%);
  border-radius: 20rpx;
  color: #FFFFFF;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}

.show-desc {
  width: 300rpx;
  margin-left: 16rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.desc-item {
  display: flex;
  flex: 1;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  color: #999999;
  font-size: 24rpx;
  padding: 0 10rpx;
}

.desc-item:nth-of-type(1) {
  border-right: 2rpx solid #E2E2E2;
}

.desc-item>text:nth-of-type(2) {
  width: 100%;
  display: inline-block;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  text-align: center;
  margin-top: 6rpx;
}

.price-sty-one {
  display: flex;
  flex-direction: column;
  margin-left: 24rpx;
}

.price-sty-one:nth-of-type(2) {
  margin-right: 8rpx !important;
}

.price-sty-one>text:nth-of-type(1) {
  font-size: 20rpx;
}

.price-desc {
  display: flex;
  justify-content: flex-start;
  align-items: flex-end;
}

.price-desc>text:nth-of-type(1) {
  font-size: 24rpx;
  margin-bottom: 6rpx;
}

.price-desc>text:nth-of-type(2) {
  font-size: 32rpx;
}

.previous-price {
  position: relative;
  font-size: 20rpx;
  height: 64rpx;
  display: flex;
  justify-content: center;
  align-items: flex-end;
}

.previous-price>text:nth-of-type(2) {
  position: absolute;
  display: inline-block;
  background: #ffffff;
  width: 100%;
  height: 2rpx;
  bottom: 19%;
  left: 0;
}

.start-sty {
  justify-content: flex-start !important;
}

.price-sty-two {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-left: 24rpx;
}

.price-sty-two>text:nth-of-type(1) {
  display: inline-block;
  height: 58rpx;
  font-size: 24rpx;
  margin-right: 12rpx;
  display: flex;
  align-items: flex-end;
  padding-bottom: 10rpx;
}

.price-desc-two {
  display: flex;
  justify-content: flex-start;
  align-items: flex-end;
  margin-right: 8rpx;
}

.price-desc-two>text:nth-of-type(1) {
  font-size: 24rpx;
  margin-bottom: 6rpx;
}

.price-desc-two>text:nth-of-type(2) {
  font-size: 40rpx;
}

.six-font-sty {
  font-size: 60rpx !important;
  margin-bottom: 4rpx;
}

.bottom-sty {
  margin-bottom: 16rpx !important;
}

.bottom-ding-sty {
  padding-bottom: 0 !important;
}

.bottom-previous-sty {
  position: relative;
  font-size: 20rpx;
  height: 60rpx;
  display: flex;
  justify-content: center;
  align-items: flex-end;
}

.bottom-previous-sty>text:nth-of-type(2) {
  position: absolute;
  display: inline-block;
  background: #ffffff;
  width: 100%;
  height: 2rpx;
  bottom: 19%;
  left: 0;
}